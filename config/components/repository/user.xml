<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE gsa-template
        PUBLIC "-//Art Technology Group, Inc.//DTD Dynamo Security//EN"
        "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
    <header>
        <name>User repository</name>
        <author>Klimkova Anastasiya</author>
        <description>
            Task 4
        </description>
    </header>
    <item-descriptor name="user" default="true">
        <table name="DPS_USER" type="primary" id-column-names="id">
        </table>
    </item-descriptor>
    <item-descriptor name="role" default="true">
        <table name="DPS_ROLE" type="primary" id-column-names="id">
        </table>
    </item-descriptor>

    <item-descriptor name="actions" default="true">
        <table name="actions" type="primary" id-column-names="id">
            <property name="visitDate" column-names="visit_date" data-type="date"/>
            <property name="isOk" column-names="isOk" data-type="boolean"/>
        </table>
    </item-descriptor>

</gsa-template>