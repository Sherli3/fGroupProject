<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE gsa-template
        PUBLIC "-//Art Technology Group, Inc.//DTD Dynamo Security//EN"
        "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
    <header>
        <name>User repository</name>
        <author>Klimkova Anastasiya</author>
        <description>Task 5</description>
    </header>
    <item-descriptor name="user" default="true">
        <table name="DPS_USER" type="primary" id-column-names="id">
            <property name="login" column-names="login" data-type="string"/>
            <property name="firstName" column-names="first_name" data-type="string"/>
            <property name="password" column-names="password" data-type="string"/>

        </table>
    </item-descriptor>
    <item-descriptor name="role">
        <table name="DPS_ROLE" type="primary" id-column-names="role_id">
            <property name="roleName" column-names="name" data-type="string"/>
        </table>
    </item-descriptor>

</gsa-template>